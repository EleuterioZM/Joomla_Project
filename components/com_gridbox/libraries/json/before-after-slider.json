{
    "desktop": {
        "title": {
            "typography": {
                "color": "@title-inverse",
                "font-family": "@default",
                "font-size": 10,
                "font-style": "normal",
                "font-weight": "700",
                "letter-spacing": 4,
                "line-height": 26,
                "text-align": "center",
                "text-decoration": "none",
                "text-transform": "uppercase"
            },
            "background": {
                "color": "@bg-dark"
            }
        },
        "divider": {
            "color": "@title-inverse",
            "width": 1
        },
        "slider": {
            "size": 24,
            "normal" : {
                "color" : "@title-inverse",
                "background" : "@primary"
            },
            "hover" : {
                "color" : "@title-inverse",
                "background" : "@hover"
            },
            "padding" : 20,
            "border" : {
                "bottom" : "0",
                "color" : "@border",
                "left" : "0",
                "right" : "0",
                "style" : "solid",
                "radius" : 50,
                "top" : "0",
                "width" : "1"
            },
            "shadow" : {
                "value" : "1",
                "color" : "@shadow"
            }
        },
        "margin" : {
            "bottom" : "25",
            "top" : "25"
        },
        "border" : {
            "bottom" : "0",
            "color" : "@border",
            "left" : "0",
            "right" : "0",
            "style" : "solid",
            "radius" : 0,
            "top" : "0",
            "width" : "1"
        },
        "shadow" : {
            "value" : "0",
            "color" : "@shadow"
        },
        "disable": "0"
    },
    "imgs": {
        "before": {
            "src": "components/com_gridbox/assets/images/thumb-landscape.png",
            "alt": "",
            "label": "Before"
        },
        "after": {
            "src": "components/com_gridbox/assets/images/thumb-landscape.png",
            "alt": "",
            "label": "After"
        }
    },
    "direction":"horizontal",
    "start": 50,
    "mouseover": true,
    "tablet": {
        "disable": "0"
    },
    "phone": {
        "disable": "0"
    },
    "type": "before-after-slider",
    "access": 1,
    "suffix": ""
}