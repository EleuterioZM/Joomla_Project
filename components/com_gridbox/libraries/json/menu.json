{
    "desktop" : {
        "margin" : {
            "bottom" : "25",
            "top" : "25"
        },
        "nav-typography" : {
            "font-family" : "@default",
            "font-size" : 12,
            "font-style" : "normal",
            "font-weight" : "500",
