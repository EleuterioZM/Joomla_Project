                "style" : "solid",
                "radius" : "0",
                "width" : "0"
            },
            "normal" : {
                "color" : "@title",
                "background" : "rgba(255,255,255,0)"
            },
            "hover" : {
                "color" : "@primary",
                "background" : "rgba(255,255,255,0)"
            }
        },
        "sub": {
            "padding" : {
                "bottom" : "20",
                "left" : "20",
                "right" : "20",
                "top" : "20"
            },
            "icon":{
                "size" : 24
            },
            "border" : {
                "bottom" : "0",
                "left" : "0",
