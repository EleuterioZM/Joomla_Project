                "top" : "0",
                "color" : "@border",
                "style" : "solid",
                "radius" : "0",
                "width" : "1"
            },
            "normal" : {
                "color" : "@title",
                "background" : "rgba(0,0,0,0)"
            },
            "hover" : {
                "color" : "@title-inverse",
                "background" : "@primary"
            }
        },
        "sub-typography" :{
            "font-family" : "@default",
            "font-size" : 12,
